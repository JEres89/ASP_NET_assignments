@model PeopleModel

@{
	ViewData["Title"] = "";
	var person = Model.GetPerson;
	var postNames = Model.postNames;
}

<div id="@person.Key" class="row">
	<div class="col personId">
		@person.Key
	</div>
	@for(int i = 0 ; i < person.Value.Length ; i++)
	{
	<div class="col @postNames[i]">
		@person.Value[i]
	</div>
	}
</div>
